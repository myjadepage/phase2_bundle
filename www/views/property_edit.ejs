<script>
     countryCodejson();//국가코드 선택  
    $('select[name="country_code"]').on("change", function(){ 
        $(this).val().replace(/\D/g,'');
    });
    timeZone(); // 타임존선택 timeZone

        //프라퍼티 상세보기
            $(function(){    
                var id = "<%= id %>";   
                var API_KEY = localStorage.getItem('api_key');  
                var url = 'properties/';         
                $.ajax({
                    //property 상세보기 api
                    url: endUrl(url) + id,                    
                    method:'put',
                    contentType: "application/json; charset=utf-8",
                    dataType:"json",
                    data:{
                        property_id : id
                    },
                    headers : headers()
                })
                .done(function(result) {    
                    $('input[name="viewName"').val(result.name);
                    $('input[name="country_code"').val(result.country_code);
                    $('input[name="viewCity"').val(result.city);
                    $('input[name="timezone"').val(result.timezone);
                    $('input[name="address"').val(result.address);
                    navTitle(result.name,result.address); 
                })
                .fail(function (request, status, error){  
                    msg = request.status + "<br>" + request.responseText + "<br>" + error;
                    console.log(msg);                           
                })    
            });
        
    </script>